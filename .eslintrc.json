{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-len":"off",
        "camelcase":"off",
        "require-jsdoc":"off",
        "no-unused-vars":"off"
    }
}
